  <body class="login-page">
    <div class="login-box">
      <div class="login-logo">
        <center><a href="/"><img src="../assets/images/logo.png" alt="LMS" class="img-responsive"></a></center>
       </div><!-- /.login-logo -->
      <div class="login-box-body">
                <!-- <b class="text-center"><u>Admin Login</u></b> <br/><b>Email:</b> admin@gmail.com <br/> <b>password:</b> admin <br/>
        <b class="text-center"><u>Member Login</u></b> <br/><b>Email:</b> member@gmail.com <br/> <b>password:</b> member<br/><hr/> -->
        <h3><p class="login-box-msg">Login</p></h3>
        <form action="#" method="post">
          <div class="form-group has-feedback">
            <input type="text" class="form-control" value="admin@gmail.com " name="username" placeholder="Email" />
            <span class="glyphicon glyphicon-user form-control-feedback"></span>
            <span style="color:red"></span>
          </div>
          <div class="form-group has-feedback">
            <input type="password" value="admin" class="form-control" name="password" placeholder="Password" />
            <span class="glyphicon glyphicon-lock form-control-feedback"></span>
            <span style="color:red"></span>
          </div>
		  
		  
          <div class="row">            
            <div class="col-xs-12">
              <button type="submit" class="btn btn-primary btn-block btn-flat">Login</button>
            </div><!-- /.col -->
          </div>
        </form>
       <br/><center><a href="forgot_password.html">Forget Password?</a></center>
      </div><!-- /.login-box-body -->
    </div><!-- /.login-box -->

